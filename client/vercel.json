{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://mwanzo-tunes-server.onrender.com/api/$1"
    },
    {
      "src": "/login",
      "dest": "https://mwanzo-tunes-server.onrender.com/login"
    },
    {
      "src": "/callback",
      "dest": "https://mwanzo-tunes-server.onrender.com/api/callback"
    },
    {
      "src": "/api/logout",
      "dest": "https://mwanzo-tunes-server.onrender.com/api/logout"
    },
    {
      "src": "/profile",
      "dest": "https://mwanzo-tunes-server.onrender.com/profile"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "https://mwanzo-tunes-server.onrender.com/uploads/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}